{
  "study_id": "4705_ross_et_al_false_consensus_effect",
  "title": "The “False Consensus Effect”: An Egocentric Bias in Social Perception and Attribution Processes",
  "authors": [
    "Lee Ross",
    "David Greene",
    "Pamela House"
  ],
  "year": 1977,
  "studies": [
    {
      "study_id": "Study 1",
      "study_name": "Questionnaire study with hypothetical situations",
      "findings": [
        {
          "finding_id": "F1",
          "main_hypothesis": "Subjects who 'choose' a particular hypothetical response will rate that response as more probable for 'people in general' than will subjects who 'choose' the alternative response.",
          "statistical_tests": [
            {
              "test_name": "Analysis of Variance (Fixed variable: Story)",
              "statistical_hypothesis": "Mean estimate of commonness by choosers > Mean estimate of commonness by non-choosers",
              "reported_statistics": "F(1, 312) = 49.1, p < .001",
              "significance_level": 0.05,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Estimates of commonness for Choice 1 by subjects who chose Choice 1 vs Choice 2 across four stories.",
            "data": {
              "supermarket_story": {
                "n_choice_1": 53,
                "percentage_choice_1": 66.0,
                "n_choice_2": 27,
                "percentage_choice_2": 34.0,
                "mean_estimate_choice_1_by_choosers_1": 75.6,
                "mean_estimate_choice_1_by_choosers_2": 57.3,
                "consensus_f": 17.7
              },
              "term_paper_story": {
                "n_choice_1": 64,
                "percentage_choice_1": 80.0,
                "n_choice_2": 16,
                "percentage_choice_2": 20.0,
                "mean_estimate_choice_1_by_choosers_1": 67.4,
                "mean_estimate_choice_1_by_choosers_2": 45.9,
                "consensus_f": 16.5
              },
              "traffic_ticket_story": {
                "n_choice_1": 37,
                "percentage_choice_1": 46.0,
                "n_choice_2": 43,
                "percentage_choice_2": 54.0,
                "mean_estimate_choice_1_by_choosers_1": 71.8,
                "mean_estimate_choice_1_by_choosers_2": 51.7,
                "consensus_f": 12.8
              },
              "space_program_referendum_story": {
                "n_choice_1": 32,
                "percentage_choice_1": 40.0,
                "n_choice_2": 48,
                "percentage_choice_2": 60.0,
                "mean_estimate_choice_1_by_choosers_1": 47.9,
                "mean_estimate_choice_1_by_choosers_2": 39.0,
                "consensus_f": 4.9
              }
            }
          }
        },
        {
          "finding_id": "F2",
          "main_hypothesis": "Subjects who 'choose' a specified response will use less extreme and less confident trait ratings in characterizing a 'typical' person making that response than will subjects who 'choose' the alternative response.",
          "statistical_tests": [
            {
              "test_name": "Analysis of Variance (Trait Ratings)",
              "statistical_hypothesis": "Difference in trait ratings for typical actors is dependent on the rater's own choice.",
              "reported_statistics": "F(1, 312) = 37.40, p < .001",
              "significance_level": 0.05,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Trait rating differences (Choice 1 - Choice 2) by subjects who chose Choice 1 vs Choice 2.",
            "data": {
              "supermarket_story": {
                "trait_rating_diff_choosers_1": -29.1,
                "trait_rating_diff_choosers_2": 24.7,
                "trait_f": 17.27
              },
              "term_paper_story": {
                "trait_rating_diff_choosers_1": -0.2,
