    {
      "study_id": "Study 3",
      "study_name": "Hypothetical sandwich board study",
      "findings": [
        {
          "finding_id": "F4",
          "main_hypothesis": "Subjects would perceive a false consensus for their own behavioral choices regardless of whether these choices were 'hypothetical'.",
          "statistical_tests": [
            {
              "test_name": "Analysis of Variance (Combined Signs)",
              "statistical_hypothesis": "Mean estimate of consensus is higher for one's own choice.",
              "reported_statistics": "F = 56.2",
              "significance_level": 0.05,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Estimates of percentage who would wear the sign by those who agreed vs refused.",
            "data": {
              "Eat at Joe's": {
                "n_wear": 27,
                "n_not_wear": 24,
                "est_wear_by_wearers": 64.6,
                "est_wear_by_refusers": 31.2,
                "consensus_f": 33.3
              },
              "Repent": {
                "n_wear": 27,
