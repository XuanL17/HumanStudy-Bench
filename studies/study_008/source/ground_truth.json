{
  "study_id": "social_norm",
  "title": "Pluralistic Ignorance and Alcohol Use on Campus: Some Consequences of Misperceiving the Social Norm",
  "authors": [
    "Deborah A. Prentice",
    "Dale T. Miller"
  ],
  "year": 1993,
  "studies": [
    {
      "study_id": "Study 1",
      "study_name": "Pluralistic Ignorance regarding Alcohol Habits",
      "findings": [
        {
          "finding_id": "F1",
          "main_hypothesis": "Students will vary in their own comfort with alcohol drinking on campus but will believe other students to be uniformly more comfortable than they are.",
          "statistical_tests": [
            {
              "test_name": "2 (sex) X 2 (target) analysis of variance (ANOVA)",
              "statistical_hypothesis": "Mean(Average Student Comfort Estimate) > Mean(Self-comfort)",
              "reported_statistics": "F(1, 130) = 55.52, p < .0001",
              "significance_level": 0.05,
              "expected_direction": "positive"
            },
            {
              "test_name": "Sex X Target interaction ANOVA",
              "statistical_hypothesis": "The difference between self and other ratings is moderated by sex.",
              "reported_statistics": "F(1, 130) = 9.96, p < .005",
              "significance_level": 0.05,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Self-comfort vs. estimated average student comfort ratings on an 11-point scale.",
            "data": {
              "Women_Self": {
                "mean": 4.68,
                "sd": 2.69,
                "n": 69
              },
              "Women_Average_Estimate": {
                "mean": 7.07,
                "sd": 1.68,
                "n": 69
              },
              "Men_Self": {
                "mean": 6.03,
                "sd": 2.76,
                "n": 63
              },
              "Men_Average_Estimate": {
                "mean": 7.0,
                "sd": 1.57,
                "n": 63
              },
              "Total_Self": {
                "mean": 5.33,
                "sd": 2.73,
                "n": 132
              },
              "Total_Average_Estimate": {
                "mean": 7.04,
                "sd": 1.63,
                "n": 132
              }
            }
          }
        },
        {
          "finding_id": "F2",
          "main_hypothesis": "Students' estimates of the variability of others' attitudes will reflect an illusion of universality (perceiving less variability in others than exists in reality).",
          "statistical_tests": [
            {
              "test_name": "Statistical comparison of variances (F-test)",
              "statistical_hypothesis": "Variance(Self-comfort) > Variance(Average Student Comfort Estimate)",
              "reported_statistics": "F(131, 131) = 2.99, p < .0001",
              "significance_level": 0.05,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Comparison of the variance in actual self-ratings versus the variance in estimates of the average student.",
            "data": {
              "Total_Self": {
                "sd": 2.73
              },
              "Total_Average_Estimate": {
                "sd": 1.63
              }
            }
          }
        }
      ]
    },
    {
      "study_id": "Study 2",
      "study_name": "Pluralistic Ignorance regarding Friends and Order Effects",
      "findings": [
        {
          "finding_id": "F1",
          "main_hypothesis": "Pluralistic ignorance will be found with respect to one's friends as well as the average student, and the effect is not a byproduct of question order.",
          "statistical_tests": [
            {
              "test_name": "2 (sex) X 3 (target) X 2 (question order) ANOVA",
              "statistical_hypothesis": "Mean(Self) < Mean(Friends) < Mean(Average Student)",
              "reported_statistics": "F(2, 476) = 54.52, p < .0001",
              "significance_level": 0.05,
              "expected_direction": "positive"
            },
            {
              "test_name": "Target X Order interaction ANOVA",
              "statistical_hypothesis": "The magnitude of the target effect depends on whether self or other is rated first.",
              "reported_statistics": "F(2, 476) = 3.45, p < .05",
              "significance_level": 0.05,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Comfort ratings for self, friends, and average student across two question-order conditions.",
            "data": {
              "Self_First_Condition_Self": {
                "mean": 5.91
              },
              "Self_First_Condition_Friend": {
                "mean": 6.61
              },
              "Self_First_Condition_Average": {
                "mean": 7.01
              },
              "Other_First_Condition_Self": {
                "mean": 5.41
              },
              "Other_First_Condition_Friend": {
                "mean": 6.49
              },
              "Other_First_Condition_Average": {
                "mean": 7.3
              }
            }
          }
        }
      ]
    },
    {
      "study_id": "Study 4",
      "study_name": "Pluralistic Ignorance and Campus Alienation regarding Keg Ban",
      "findings": [
        {
          "finding_id": "F1",
          "main_hypothesis": "Feeling deviant from the perceived norm (pluralistic ignorance) will inhibit students from taking social action.",
          "statistical_tests": [
            {
              "test_name": "ANCOVA on willingness to collect signatures (controlling for attitudes)",
              "statistical_hypothesis": "Mean(Others-the-same Signatures) > Mean(Others-more-negative Signatures)",
              "reported_statistics": "F(1, 90) = 18.94, p < .0001",
              "significance_level": 0.05,
              "expected_direction": "positive"
            },
            {
              "test_name": "ANCOVA on willingness to work hours (controlling for attitudes)",
              "statistical_hypothesis": "Mean(Others-the-same Hours) > Mean(Others-more-negative Hours)",
              "reported_statistics": "F(1, 90) = 10.99, p < .005",
              "significance_level": 0.05,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Willingness to engage in social action (signatures and hours) based on perceived deviance from the norm.",
            "data": {
              "Others_more_negative_Women_Signatures": {
                "mean": 6.05
              },
              "Others_the_same_Women_Signatures": {
                "mean": 49.09
              },
              "Others_more_negative_Women_Hours": {
                "mean": 0.4
              },
              "Others_the_same_Women_Hours": {
                "mean": 2.55
              }
            }
          }
        },
        {
          "finding_id": "F2",
          "main_hypothesis": "Feeling deviant from the perceived norm will produce symptoms of alienation from the university.",
          "statistical_tests": [
            {
              "test_name": "ANCOVA on percentage of reunions expected (controlling for attitudes)",
              "statistical_hypothesis": "Mean(Others-the-same Reunions) > Mean(Others-more-negative Reunions)",
              "reported_statistics": "F(1, 89) = 8.10, p < .01",
              "significance_level": 0.05,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Expected reunion attendance as a measure of institutional connection/alienation.",
            "data": {
              "Others_more_negative_Women_Reunions": {
                "mean": 33.88
              },
              "Others_the_same_Women_Reunions": {
                "mean": 57.27
              }
            }
          }
        }
      ]
    }
  ],
