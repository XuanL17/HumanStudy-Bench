    "AND MARTIN SEFTON"
  ],
  "year": 1994,
  "studies": [
    {
      "study_id": "Experiment 1",
      "study_name": "$5 Dictator Game with Pay (DG-P)",
      "findings": [
        {
          "finding_id": "F1",
          "main_hypothesis": "The distribution of offers is the same across different experimental sessions (April vs. September).",
          "statistical_tests": [
            {
              "test_name": "Anderson-Darling (AD) test",
              "statistical_hypothesis": "Distribution(April) = Distribution(September)",
              "reported_statistics": "AD=0.58, p=0.66, n=45",
              "significance_level": 0.05,
              "expected_direction": "none"
            },
            {
              "test_name": "Characteristic Function (CF) test",
              "statistical_hypothesis": "Distribution(April) = Distribution(September)",
              "reported_statistics": "CF=1.14, p=0.89, n=45",
              "significance_level": 0.05,
              "expected_direction": "none"
            }
          ],
          "original_data_points": {
            "description": "Comparison of offers between April and September sessions for the $5 Dictator Game with Pay.",
            "data": {
              "April Session Offers ($)": {
                "mean": 1.0,
                "sd": 1.01,
                "n": 21
              },
              "September Session Offers ($)": {
                "mean": 1.17,
                "sd": 0.99,
                "n": 24
              }
            }
          }
        },
        {
          "finding_id": "F2",
          "main_hypothesis": "The distribution of offers in the Dictator Game is identical to the distribution of offers in the Ultimatum Game.",
          "statistical_tests": [
            {
              "test_name": "Anderson-Darling (AD) test",
              "statistical_hypothesis": "Distribution(DG-P) = Distribution(UG-P)",
              "reported_statistics": "AD=13.00, p=0.00, n=88",
              "significance_level": 0.05,
              "expected_direction": "negative"
            },
            {
              "test_name": "Characteristic Function (CF) test",
              "statistical_hypothesis": "Distribution(DG-P) = Distribution(UG-P)",
              "reported_statistics": "CF=43.58, p=0.00, n=88",
              "significance_level": 0.05,
              "expected_direction": "negative"
            }
          ],
          "original_data_points": {
            "description": "Comparison of pooled offers between Dictator and Ultimatum games with pay.",
            "data": {
              "Pooled $5 DG-P": {
                "mean": 1.09,
                "sd": 0.99,
                "n": 45
              },
              "Pooled $5 UG-P": {
                "mean": 2.33,
                "sd": 0.49,
                "n": 43
              }
            }
          }
        }
      ]
    },
