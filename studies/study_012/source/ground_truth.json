{
  "study_id": "berg_geb95",
  "title": "Trust, Reciprocity, and Social History",
  "authors": [
    "Joyce Berg",
    "John Dickhaut",
    "Kevin McCabe"
  ],
  "year": 1995,
  "studies": [
    {
      "study_id": "Experiment 1",
      "study_name": "No History Treatment",
      "findings": [
        {
          "finding_id": "F1",
          "main_hypothesis": "Trust exists (A0: Ma > 0 for some a) and the distribution of amounts sent is not uniform.",
          "statistical_tests": [
            {
              "test_name": "Approximate Randomization Test",
              "statistical_hypothesis": "The sample of Room A decisions was drawn from a uniform distribution (N0).",
              "reported_statistics": "p = 0.29",
              "significance_level": 0.05,
              "expected_direction": "none"
            }
          ],
          "original_data_points": {
            "description": "Investment decisions (amount sent) by Room A subjects.",
            "data": {
              "No History Room A": {
                "mean": 5.16,
                "n": 32
              }
            }
          }
        },
        {
          "finding_id": "F2",
          "main_hypothesis": "Reciprocity exists as a socially stable norm (A2: Average kb(3Ma) > Ma).",
          "statistical_tests": [
            {
              "test_name": "Mean Comparison",
              "statistical_hypothesis": "Average amount returned (kb(3Ma)) > Average amount sent (Ma).",
              "reported_statistics": "Mean Sent = $5.16, Mean Returned = $4.66",
              "significance_level": null,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Comparison of average investment vs average payback.",
            "data": {
              "Room A (Sent)": {
                "mean": 5.16,
                "n": 32
              },
              "Room B (Returned)": {
                "mean": 4.66,
                "n": 32
              }
            }
          }
        },
        {
          "finding_id": "F3",
          "main_hypothesis": "Amounts sent and payback decisions are positively correlated (A3).",
          "statistical_tests": [
            {
              "test_name": "Spearman's rank correlation coefficient",
              "statistical_hypothesis": "Correlation (rs) between Ma and kb(Ma) > 0.",
              "reported_statistics": "rs = 0.01",
              "significance_level": null,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Correlation between the amount sent by Room A and the amount returned by Room B.",
            "data": {
              "No History Pairs": {
                "n": 32,
                "correlation": 0.01
              }
            }
          }
        }
      ]
    },
    {
      "study_id": "Experiment 2",
      "study_name": "Social History Treatment",
      "findings": [
        {
          "finding_id": "F4",
          "main_hypothesis": "Trust exists and is influenced by social history, leading to a non-uniform distribution of amounts sent.",
          "statistical_tests": [
            {
              "test_name": "Approximate Randomization Test",
              "statistical_hypothesis": "The sample of Room A decisions was drawn from a uniform distribution (N0).",
              "reported_statistics": "p = 0.06",
              "significance_level": 0.05,
              "expected_direction": "none"
            }
          ],
          "original_data_points": {
            "description": "Investment decisions by Room A subjects provided with social history.",
            "data": {
              "Social History Room A": {
                "mean": 5.36,
                "n": 28
              }
            }
          }
        },
        {
          "finding_id": "F5",
          "main_hypothesis": "Social history increases the level of reciprocity (paybacks) compared to the No History treatment.",
          "statistical_tests": [
            {
              "test_name": "Wilcoxon rank-sum test",
              "statistical_hypothesis": "Paybacks in Social History > Paybacks in No History.",
              "reported_statistics": "r = 776, p = 0.1",
              "significance_level": 0.1,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Comparison of payback amounts between Experiment 1 and Experiment 2.",
            "data": {
              "No History Payback": {
                "mean": 4.66,
                "n": 32
              },
              "Social History Payback": {
                "mean": 6.46,
                "n": 28
              }
            }
          }
        },
        {
          "finding_id": "F6",
          "main_hypothesis": "Social history strengthens the correlation between trust and reciprocity.",
          "statistical_tests": [
            {
              "test_name": "Spearman's rank correlation coefficient",
              "statistical_hypothesis": "Correlation (rs) in Social History > 0.",
              "reported_statistics": "rs = 0.34",
              "significance_level": null,
              "expected_direction": "positive"
            },
            {
              "test_name": "Resampling test",
              "statistical_hypothesis": "Correlation (Social History) > Correlation (No History).",
              "reported_statistics": "p = 0.06",
              "significance_level": 0.1,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Correlation between amount sent and amount returned in the Social History condition.",
            "data": {
              "Social History Pairs": {
                "n": 28,
                "correlation": 0.34
              }
            }
          }
        }
      ]
    },
    {
      "study_id": "Combined Analysis",
      "study_name": "Joint Earnings & Behavioral Types",
      "findings": [
        {
          "finding_id": "F7",
          "main_hypothesis": "Room B's behavior follows specific fairness norms or strategies (k=0, k=1/3, k=1/2, k=2/3).",
          "statistical_tests": [
            {
              "test_name": "Strategy Type Classification",
              "statistical_hypothesis": "Over 90% of data points can be explained by four strategy types (k=0, k=1/3, k=1/2, k=2/3) within $1 tolerance.",
              "reported_statistics": ">90% of pairs follow one of four strategies",
              "significance_level": null,
              "expected_direction": "positive"
            }
          ],
          "original_data_points": {
            "description": "Classification of Room B return strategies based on k = Return / (3 * Sent).",
            "data": {
              "Strategy Types": {
                "k=0": "Selfish (Return = 0)",
                "k=1/3": "Return principal (Return = Sent)",
                "k=1/2": "Split total (Return = 1.5 * Sent)",
                "k=2/3": "Equal earnings (Return makes final earnings equal)"
              },
              "Coverage": ">90% of 60 pairs"
            }
          }
        }
      ]
    }
  ],
